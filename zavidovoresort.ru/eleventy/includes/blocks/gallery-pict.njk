<div class="gallery-pict">
  <div class="container">
    <h1 class="gallery-pict__title title">Фотогалерея</h1>
    <div class="gallery-pict__unit">
      {% for item in params.galleryPict %}
        <div class="gallery-pict__item">
          <a data-fancybox="gallery" href="/assets/images/{{ item.images.img }}">
            <picture class="gallery-pict__picture">
              <source
            srcset="/assets/images/{{ item.images.webp }}, /assets/images/{{ item.images.webp2x }} 2x"
            type="image/webp"
/>
              <img
            class="gallery-pict__img"
            srcset="/assets/images/{{ item.images.img }}, /assets/images/{{ item.images.img2x }} 2x"
            src="/assets/images/{{ item.images.img }}"
            loading="lazy"
            alt="house"
            width="335"
            height="335"
/>
            </picture>
          </a>
        </div>
      {% endfor %}
    </div>
  </div>
</div>